{
  "name": "factionfun-backend",
  "version": "0.1.0",
  "description": "On-chain group chat for top token holders",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "node --dns-result-order=ipv4first --loader ts-node/esm src/index.ts",
    "build": "tsc",
    "start": "node --dns-result-order=ipv4first dist/index.js",
    "db:migrate": "node --loader ts-node/esm src/db/migrate.ts",
    "db:seed": "node --loader ts-node/esm src/db/seed.ts",
    "lint": "eslint src --ext .ts",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "solana",
    "chat",
    "holders",
    "token"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@privy-io/node": "^0.9.0",
    "@solana/spl-token": "^0.3.8",
    "@solana/web3.js": "^1.87.6",
    "axios": "^1.6.1",
    "bull": "^4.11.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^8.1.0",
    "pg": "^8.10.0",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/bull": "^4.10.4",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/express-serve-static-core": "^5.1.1",
    "@types/node": "^20.10.6",
    "@types/pg": "^8.16.0",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "eslint": "^8.56.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
